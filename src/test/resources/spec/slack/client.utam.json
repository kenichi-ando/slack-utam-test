{
    "root": true,
    "selector": {
        "css": ".p-client"
    },
    "elements": [
        {
            "name": "topNavSearch",
            "selector": {
                "css": "[data-qa='top_nav_search']"
            },
            "type": [
                "clickable"
            ],
            "public": true
        },
        {
            "name": "searchResults",
            "selector": {
                "css": "[data-qa='search_result']",
                "returnAll": true
            }
        },
        {
            "name": "searchPageCloseButton",
            "selector": {
                "css": "[data-qa='search_page_close_button']"
            },
            "type": [
                "clickable"
            ],
            "public": true
        },
        {
            "name": "messageInput",
            "selector": {
                "css": "[data-qa='message_input'] .ql-editor"
            },
            "type": [
                "editable"
            ],
            "public": true
        },
        {
            "name": "textSendButton",
            "selector": {
                "css": "[data-qa='texty_send_button']"
            },
            "type": [
                "clickable"
            ],
            "public": true
        },
        {
            "name": "messageTexts",
            "selector": {
                "css": "[data-qa='message-text']",
                "returnAll": true
            },
            "public": true
        }
    ],
    "methods": [
        {
            "name": "waitForSearchResults",
            "compose": [
                {
                    "apply": "waitFor",
                    "args": [
                        {
                            "type": "function",
                            "predicate": [
                                {
                                    "element": "searchResults",
                                    "apply": "isVisible"
                                }
                            ]
                        }
                    ]
                },
                {
                    "element": "searchResults"
                }
            ]
        }
    ]
}