{
    "root": true,
    "selector": {
        "css": "#signin_form"
    },
    "elements": [
        {
            "name": "email",
            "selector": {
                "css": "#email"
            },
            "type": [
                "editable"
            ]
        },
        {
            "name": "password",
            "selector": {
                "css": "#password"
            },
            "type": [
                "editable"
            ]
        },
        {
            "name": "signin",
            "selector": {
                "css": "#signin_btn"
            },
            "type": [
                "clickable"
            ]
        }
    ],
    "methods": [
        {
            "name": "login",
            "compose":[
                {
                    "element": "email",
                    "apply": "clearAndType",
                    "args": [
                        {
                            "name": "email",
                            "type": "string"
                        }
                    ]
                },
                {
                    "element": "password",
                    "apply": "clearAndType",
                    "args": [
                        {
                            "name": "password",
                            "type": "string"
                        }
                    ]
                },
                {
                    "element": "signin",
                    "apply": "click"
                }
            ]
        }
    ]
}