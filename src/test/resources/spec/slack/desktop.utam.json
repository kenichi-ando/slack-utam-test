{
  "root": true,
  "selector": {
    "css": "body"
  },
  "elements": [
    {
      "name": "client",
      "selector": {
        "css": ".p-client"
      },
      "elements": [
        {
          "name": "topNavSearch",
          "selector": {
            "css": "[data-qa='top_nav_search']"
          },
          "type": [
            "clickable"
          ],
          "public": true
        },
        {
          "name": "searchResults",
          "selector": {
            "css": "[data-qa='search_result']",
            "returnAll": true
          },
          "public": true
        },
        {
          "name": "searchPageCloseButton",
          "selector": {
            "css": "[data-qa='search_page_close_button']"
          },
          "type": [
            "clickable"
          ],
          "public": true
        },
        {
          "name": "generalChannel",
          "selector": {
            "css": "[data-qa='channel_sidebar_name_general']"
          },
          "type": [
            "clickable"
          ],
          "public": true
        },
        {
          "name": "messageInput",
          "selector": {
            "css": "[data-qa='message_input'] .ql-editor"
          },
          "type": [
            "editable"
          ],
          "public": true
        },
        {
          "name": "textSendButton",
          "selector": {
            "css": "[data-qa='texty_send_button']"
          },
          "type": [
            "clickable"
          ],
          "public": true
        },
        {
          "name": "messageTexts",
          "selector": {
            "css": "[data-qa='message-text']",
            "returnAll": true
          },
          "public": true
        }
      ]
    },
    {
      "name": "searchInput",
      "selector": {
        "css": ".c-search_modal__wrapper [data-qa='focusable_search_input'] .ql-editor"
      },
      "type": [
        "editable"
      ],
      "public": true
    }
  ],
  "beforeLoad": [
    {
      "apply": "waitFor",
      "args": [
        {
          "type": "function",
          "predicate": [
            {
              "element": "root",
              "apply": "isPresent"
            },
            {
              "element": "root",
              "apply": "containsElement",
              "args": [
                {
                  "type": "locator",
                  "value": {
                    "css": ".p-client"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}