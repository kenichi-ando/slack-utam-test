{
  "root": true,
  "selector": {
    "css": "body"
  },
  "elements": [
    {
      "name": "client",
      "selector": {
        "css": ".p-client"
      },
      "type": "utam-slack/pageObjects/client",
      "public": true
    },
    {
      "name": "searchModal",
      "selector": {
        "css": ".c-search_modal__wrapper"
      },
      "type": "utam-slack/pageObjects/search",
      "public": true
    }
  ],
  "beforeLoad": [
    {
      "apply": "waitFor",
      "args": [
        {
          "type": "function",
          "predicate": [
            {
              "element": "root",
              "apply": "isPresent"
            },
            {
              "element": "root",
              "apply": "containsElement",
              "args": [
                {
                  "type": "locator",
                  "value": {
                    "css": ".p-client"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}