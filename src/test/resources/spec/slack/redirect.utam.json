{
  "root": true,
  "selector": {
    "css": "[data-qa='ssb_redirect_loading_page']"
  },
  "elements": [
    {
      "name": "link",
      "selector": {
        "css": ".p-ssb_redirect__loading_messages .c-link"
      },
      "type": [
        "clickable"
      ]
    }
  ],
  "methods": [
    {
      "name": "waitForLink",
      "compose": [
        {
          "apply": "waitFor",
          "args": [
            {
              "type": "function",
              "predicate": [
                {
                  "element": "link",
                  "apply": "isVisible"
                }
              ]
            }
          ]
        },
        {
          "element": "link"
        }
      ]
    }
  ]
}